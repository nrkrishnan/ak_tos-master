<?xml version="1.0"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd" version="2.1"> 
 <enterprise-beans>
 
    <message-driven>
      <ejb-name>MdbGemsTos</ejb-name>
      <ejb-class>com.matson.tos.processor.MdbGemsMessageProcessor</ejb-class>
      <transaction-type>Container</transaction-type>
      <message-destination-type>javax.jms.Queue</message-destination-type>
      <activation-config>
        <activation-config-property>
          <activation-config-property-name>acknowledgeMode</activation-config-property-name>
          <activation-config-property-value>Auto-acknowledge</activation-config-property-value>
        </activation-config-property>
      </activation-config>
    </message-driven>
	<message-driven>
      <ejb-name>MdbSn4Out</ejb-name>
      <ejb-class>com.matson.tos.processor.MdbSn4MessageProcessor</ejb-class>
      <transaction-type>Container</transaction-type>
       <message-destination-type>javax.jms.Queue</message-destination-type>
       <activation-config>
        <activation-config-property>
          <activation-config-property-name>acknowledgeMode</activation-config-property-name>
          <activation-config-property-value>Auto-acknowledge</activation-config-property-value>
        </activation-config-property>
      </activation-config>
    </message-driven>
     <message-driven>
         <ejb-name>MdbSn4OutKDK</ejb-name>
         <ejb-class>com.matson.tos.processor.MdbSn4MessageProcessor</ejb-class>
         <transaction-type>Container</transaction-type>
         <message-destination-type>javax.jms.Queue</message-destination-type>
         <activation-config>
             <activation-config-property>
                 <activation-config-property-name>acknowledgeMode</activation-config-property-name>
                 <activation-config-property-value>Auto-acknowledge</activation-config-property-value>
             </activation-config-property>
         </activation-config>
     </message-driven>
     <message-driven>
         <ejb-name>MdbSn4OutDUT</ejb-name>
         <ejb-class>com.matson.tos.processor.MdbSn4MessageProcessor</ejb-class>
         <transaction-type>Container</transaction-type>
         <message-destination-type>javax.jms.Queue</message-destination-type>
         <activation-config>
             <activation-config-property>
                 <activation-config-property-name>acknowledgeMode</activation-config-property-name>
                 <activation-config-property-value>Auto-acknowledge</activation-config-property-value>
             </activation-config-property>
         </activation-config>
     </message-driven>
	<message-driven>
      <ejb-name>ManifestReport</ejb-name>
      <ejb-class>com.matson.tos.processor.MdbManifestMessageProcessor</ejb-class>
      <transaction-type>Container</transaction-type>
      <message-destination-type>javax.jms.Topic</message-destination-type>
     <activation-config>
        <activation-config-property>
          <activation-config-property-name>acknowledgeMode</activation-config-property-name>
          <activation-config-property-value>Auto-acknowledge</activation-config-property-value>
        </activation-config-property>
      </activation-config>
    </message-driven>    
 	 <message-driven>
      <ejb-name>FSSPublish</ejb-name>
      <ejb-class>com.matson.tos.processor.MdbFSSMessageProcessor</ejb-class>
      <transaction-type>Bean</transaction-type>
       <message-destination-type>javax.jms.Topic</message-destination-type>
       <activation-config>
        <activation-config-property>
          <activation-config-property-name>acknowledgeMode</activation-config-property-name>
          <activation-config-property-value>Auto-acknowledge</activation-config-property-value>
        </activation-config-property>
      </activation-config>
    </message-driven>       

 </enterprise-beans>
</ejb-jar>

