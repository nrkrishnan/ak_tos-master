        <meta attribute="extends">com.navis.framework.persistence.HibernatingEntity</meta>
        <meta attribute="class-description">Bill of Lading Cargo Lot</meta>
        <meta attribute="n4-entity-name">Archived Bill of Lading Cargo Lot</meta>
        <meta attribute="n4-entity-name-plural">Archived Bill of Lading Cargo Lots</meta>
