            create index crg_ar_lot_arr_pos_anchor_idx on crg_ar_lots(arrive_pos_anchor) INDEXTYPE IS MDSYS.SPATIAL_INDEX;
        </meta>
        <meta attribute="carina-sqlserver-index">
            create spatial index crg_ar_lot_arr_pos_anchor_idx on crg_ar_lots(arrive_pos_anchor) with ( bounding_box = ( 0, 0, 1000000, 1000000 ) );
        </meta>

        <id name="arLotGkey" column="gkey" type="java.lang.Long" unsaved-value="null">
            <generator class="native"/>
        </id>

        <property name="arLotOriginalGkey" column="original_gkey" type="java.lang.Long">
            <meta attribute="field-description">gkey of the original entity before it's been purged/archived.</meta>
        </property>

        <property name="arLotArchiveDate" column="archived_date" type="timestamp" not-null="true">
            <meta attribute="field-description">The time this unit was archived.</meta>
        </property>

        <property name="arLotSubclass" column="lot_type" not-null="false" type="java.lang.String" length="10"/>

        <!-- we are going to persist ids for all type of lots for easy access -->
        <property name="arLotId" column="Id" type="java.lang.String" length="60">
            <meta attribute="field-description">Identifier for this piece of cargo</meta>
        </property>

        <property name="arLotIsDefaultLot" column="is_default_lot" type="java.lang.Boolean">
            <meta attribute="field-description">Default (Generic) Lot</meta>
        </property>

        <set name="arLotDamages" lazy="true" inverse="true" table="crg_ar_damages" cascade="all-delete-orphan">
            <key column="crglot_gkey"/>
            <one-to-many class="com.navis.cargo.business.model.ArchiveCargoDamage"/>
        </set>

        <property name="arLotArBlitemInboundGkey" column="blitem_ib_gkey" type="java.lang.Long" not-null="true">
            <meta attribute="field-description">gkey of original Inbound BL Item for this cargo lot</meta>
        </property>

        <property name="arLotArBlitemInboundNbr" column="blitem_ib_nbr" type="java.lang.String">
            <meta attribute="field-description">Nbr of Inbound BL Item for this cargo lot</meta>
        </property>

        <property name="arLotArBlitemOutboundGkey" column="blitem_ob_gkey" type="java.lang.Long" not-null="false">
            <meta attribute="field-description">gkey of original Outbound BL Item for this cargo lot</meta>
        </property>

        <property name="arLotArBlitemOutboundNbr" column="blitem_ob_nbr" type="java.lang.String">
            <meta attribute="field-description">Nbr of Outbound BL Item for this cargo lot</meta>
        </property>

        <property name="arLotSeqNbr" column="seq_nbr" type="java.lang.Integer">
            <meta attribute="field-description">Sequence of this lot amongst all lots for the same BL Item</meta>
        </property>

        <property name="arLotExportReleaseAliasId" column="export_release_alias_id" type="string" length="40">
            <meta attribute="field-description">Export Release identifier - additional information to identify the release information</meta>
        </property>

        <property name="arLotQuantity" column="quantity" type="java.lang.Double">
            <meta attribute="field-description">Quantity currently at this location</meta>
        </property>

        <property name="arLotQuantityManifiested" column="quantity_manifested" type="java.lang.Double">
            <meta attribute="field-description">Quantity originally declared at this location</meta>
        </property>

        <property name="arLotQuantityDiscrepency" column="quantity_discrepency" type="java.lang.Double">
            <meta attribute="field-description">Quantity missing (-) or overage (+) at this location</meta>
        </property>

        <property name="arLotWeightTotalKg" column="weight_total_kg" type="java.lang.Double">
            <meta attribute="field-description">Total weight of lot.</meta>
        </property>

        <property name="arLotMnftCargoSeqNo" column="mnft_cargo_seq_nbr" type="string" length="50">
            <meta attribute="field-description">Cargo sequence number received in manifest edi</meta>
        </property>

        <property name="arLotExportReleaseNbr" column="export_release_nbr" type="string" length="10">
            <meta attribute="field-description">Export Release Number</meta>
        </property>

        <many-to-one name="arLotGoods" column="goods_gkey" class="com.navis.inventory.business.units.ArchiveGoodsBase"
                     cascade="none">
            <meta attribute="field-description">Associated Unit Goods</meta>
        </many-to-one>

        <!-- The Brand, Model and Color specified on the BL ITEM can be overridden for each piece -->

        <property name="arLotBrand" column="brand" type="java.lang.String" length="60">
            <meta attribute="field-description">Brand name of the lot</meta>
        </property>

        <property name="arLotModel" column="model" type="java.lang.String" length="60">
            <meta attribute="field-description">Model name of the lot</meta>
        </property>

        <property name="arLotColor" column="color" type="java.lang.String" length="60">
            <meta attribute="field-description">Color of the lot</meta>
        </property>

        <property name="arLotDmgSeverity" column="dmg_severity" type="com.navis.cargo.persistence.atoms.UserTypeCargoDamageSeverityEnum">
            <meta attribute="field-description">Damage Severity</meta>
        </property>

        <property name="arLotPlannedPosition" column="planned_position" type="java.lang.String" length="60">
            <meta attribute="field-description">General lot planning position</meta>
        </property>

        <component name="arLotPosition" class="com.navis.argo.business.model.ArchiveLocPosition">
            <meta attribute="generated-class">com.navis.argo.business.model.LocPositionHbr</meta>
            <meta attribute="field-description">The last known position of the Unit from the perspective of this facility</meta>
            <meta attribute="extends">com.navis.framework.persistence.HibernatingComponent</meta>

            <property name="arPosLocType" column="pos_loctype" type="com.navis.argo.persistence.atoms.UserTypeLocTypeEnum" length="16"
                      not-null="false">
                <meta attribute="field-description">An Enum giving the type of position</meta>
            </property>
            <property name="arPosLocId" column="pos_locid" type="string" length="16">
                <meta attribute="field-description">A String giving the Id of the location: either a yard code, or carrier visit Id</meta>
            </property>
            <property name="arPosLocGkey" column="pos_loc_gkey" type="java.lang.Long" index="lot_unfcyvis_lplgkey_idx">
                <meta attribute="field-description">The gkey of the Yard or CarrierVisit of this Position</meta>
            </property>
            <property name="arPosSlot" column="pos_slot" type="string" length="16">
                <meta attribute="field-description">A String giving the coordinates of the Position within its Location</meta>
            </property>
            <property name="arPosOrientation" column="pos_orientation" type="string" length="6">
                <meta attribute="field-description">A String defining the orientation of the Unit within this Position</meta>
            </property>
            <property name="arPosName" column="pos_name" type="string" length="42" index="lot_unit_fcy_vst_lastposnm_idx">
                <!--42 = 1 + 16 + 16 + 6 + 3 separators -->
                <meta attribute="field-description">A String formatted for the user that represents the position</meta>
            </property>
            <property name="arPosOperationalPosId" column="ops_pos_id" type="string" length="80">
                <meta attribute="field-description">XPS operation position (TIP, TQ, LN.4, etc.)</meta>
            </property>
            <!-- 2009-10-30 pji Support persistent bin model. -->
            <many-to-one name="arPosBin" column="arrive_pos_bin" class="com.navis.spatial.business.model.AbstractBin">
                 <meta attribute="field-description">The bin to which this Position points to.</meta>
            </many-to-one>
            <property name="arPosTier" column="arrive_pos_tier" type="java.lang.Long">
                <meta attribute="field-description">An integer specifying the tier for this Position.</meta>
            </property>
            <property name="arPosAnchor" column="arrive_pos_anchor" type="org.hibernatespatial.GeometryUserType">
                <meta attribute="field-description">xyz anchor point in bin's coordinate system.</meta>
            </property>
            <property name="arPosOrientationDegrees" column="arrive_pos_orientation_degrees" type="java.lang.Double">
                <meta attribute="field-description">Orientation in degrees in bin's coordinate system.</meta>
            </property>
         </component>

        <property name="arLotCreated" column="created" type="timestamp"/>
